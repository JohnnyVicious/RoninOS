[Unit]
Description=Post Setup RoninDojo
After=ronin-setup.service network-online.target
Wants=network-online.target

[Service]
Type=simple
User=ronindojo
WorkingDirectory=/home/ronindojo
ExecStart=/usr/bin/pm2 resurrect
StandardOutput=journal
RemainAfterExit=true

[Install]
WantedBy=multi-user.target